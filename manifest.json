{
  "manifest_version": 3,
  "name": "Insta Stories Downloader",
  "description": "This extension allows the user to download Instagram Stories.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
